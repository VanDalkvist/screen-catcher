<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
   
    <SolidColorBrush x:Key="WhiteBrush" Color="White"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="#FF0C0A0A"/>
    <SolidColorBrush x:Key="GrayBrush" Color="DarkGray"/>
    <SolidColorBrush x:Key="DisabledBrush" Color="DimGray" />
    <SolidColorBrush x:Key="MouseOverBrush" Color="#FFF3A871" />
    <SolidColorBrush x:Key="ActiveBrush" Color="Chocolate" />
    <SolidColorBrush x:Key="NormalBrush" Color="#FFF1CCB1" />

</ResourceDictionary>