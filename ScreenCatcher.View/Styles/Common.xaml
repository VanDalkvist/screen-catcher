<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="WhiteColor">White</Color>
    <Color x:Key="ForegroundColor">#FF0C0A0A</Color>
    <Color x:Key="GrayColor">DarkGray</Color>
    <Color x:Key="DisabledColor">DimGray</Color>
    <Color x:Key="MouseOverColor">#FFF3A871</Color>
    <Color x:Key="ActiveColor">Chocolate</Color>
    <Color x:Key="NormalColor">#FFF1CCB1</Color>
    
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource GrayColor}"/>
    <SolidColorBrush x:Key="DisabledBrush" Color="{StaticResource DisabledColor}" />
    <SolidColorBrush x:Key="MouseOverBrush" Color="{StaticResource MouseOverColor}" />
    <SolidColorBrush x:Key="ActiveBrush" Color="{StaticResource ActiveColor}" />
    <SolidColorBrush x:Key="NormalBrush" Color="{StaticResource NormalColor}" />

</ResourceDictionary>